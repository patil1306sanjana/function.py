#function without return and without parameter

def sum():
   print("Enter 2 numbers")
   a = int(input())
   b = int(input())
   s = a + b
   print("sum is",s)
 
sum()

#function with parameter and without return

def sum(a,b):
   s = a + b
   print("sum is",s)
print("Enter 2 numbers")
a = int(input())
b = int(input())
sum(a,b)

#function with return and without parameter
def sum():
   print("Enter 2 numbers")
   a = int(input())
   b = int(input())
   s = a + b
   return s

c = sum()
print("sum is",c)